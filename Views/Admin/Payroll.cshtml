@model List<PayrollEntry>
<h3 class="text-white">Nómina</h3>
<p class="text-white-50">Este módulo mostrará actualizaciones de nómina cargadas y validadas.</p>
<table class="table table-dark"><thead><tr><th>Código</th><th>Cédula</th><th>Nombre</th><th>Cargo</th><th>Actualizado</th></tr></thead><tbody>@foreach(var n in Model){<tr><td>@n.EmployeeCode</td><td>@n.Cedula</td><td>@n.FullName</td><td>@n.Position</td><td>@n.UpdatedAtUtc</td></tr>}</tbody></table>
